{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/frontend/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/frontend/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1",
      "check": true
    },
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "/frontend/index.html"
    }
  ]
}
